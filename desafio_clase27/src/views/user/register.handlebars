<section class="form-container">
  <article>
    <h1>Registro</h1>
    <form method="POST" class="user_form" action="" id="register">
      <div>
        <label for="first_name">First Name</label>
        <input type="text" name="first_name" id="first_name" autocomplete="off" />
      </div>
      <div>
        <label for="last_name">Last Name</label>
        <input type="text" name="last_name" id="last_name" autocomplete="off" />
      </div>
      <div>
        <label for="email">Email</label>
        <input type="email" name="email" id="email" autocomplete="off" />
      </div>
      <div>
        <label for="age">Age</label>
        <input type="number" name="age" id="age" autocomplete="off" min="18" max="100" />
      </div>
      <div>
        <label for="password">Password</label>
        <input type="password" name="password" id="password" autocomplete="new-password" />
      </div>
      <button class="btn-primary">Create Account</button>
      <p>Already have an account? <a href="/login">Sign In</a></p>
    </form>
  </article>
</section>

<script src="/js/user.js"></script>