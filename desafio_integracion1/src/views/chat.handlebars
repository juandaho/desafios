<div id="chat-container">
    <div class="chat-header">
        <h4 id="nombreusuario">Username</h4>
    </div>
    <div id="chat-messages" class="chat-messages">
        <div class="message sender">
            <p id="chat"></p>
        </div>
    </div>
    <form id="formulario" class="chat-form">
        <input class="chat-input" type="text" name="mensaje" id="mensaje" placeholder="Type your message...">
        <button class="chat-button" type="submit">Enviar</button>
    </form>
</div>

<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/toastify-js"></script>

<script src="/socket.io/socket.io.js"></script>
<script src="js/chat.js"></script>
